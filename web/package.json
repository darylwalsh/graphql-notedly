{
  "name": "notedly-web",
  "version": "0.1.0",
  "description": "React front-end",
  "main": "src/index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "final": "parcel final/index.html",
    "deploy:src": "parcel build src/index.html --public-url ./",
    "deploy:final": "parcel build final/index.html --public-url ./",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./src/index.html --open",
    "build": "npm run type-check && parcel build ./src/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javascripteverywhere/web.git"
  },
  "keywords": [],
  "author": "Adam Scott <adam@jseverywhere.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javascripteverywhere/web/issues"
  },
  "homepage": "https://github.com/javascripteverywhere/web#readme",
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "a11y-react-emoji": "^1.1.1",
    "apollo-boost": "0.4.4",
    "apollo-client": "2.6.4",
    "date-fns": "1.30.1",
    "graphql": "14.5.8",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-inlinesvg": "^1.2.0",
    "react-markdown": "4.2.2",
    "react-router-dom": "5.1.2",
    "react-svg": "^11.0.4",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "5.0.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-core": "6.26.3",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
